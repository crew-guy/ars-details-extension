{
	"compilerOptions": {
		"forceConsistentCasingInFileNames": true,
		"importHelpers": true,
		"removeComments": true,
		"strict": true,
		"baseUrl": "./",
		"paths": {
			"@components/*": ["./src/components/*"],
			"@pages/*": ["./src/pages/*"],
			"@features/*": ["./src/redux/features/*"],
			"@actions/*": ["./src/redux/actions/*"],
			"@redux/*": ["./src/redux/*"],
			"@styles/*": ["./src/assets/css/*"],
			"@images/*": ["./src/assets/images"]
		}
	}
}
